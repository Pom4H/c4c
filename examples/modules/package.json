    "test:client": "tsx scripts/test-client.ts"
  },
  "dependencies": {
    "@c4c/core": "workspace:*",
    "@c4c/adapters": "workspace:*",
    "@c4c/policies": "workspace:*",
    "@c4c/generators": "workspace:*",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@c4c/cli": "workspace:*",
    "@types/node": "^24.7.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
